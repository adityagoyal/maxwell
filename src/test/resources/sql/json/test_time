create database timedb
use timedb

set global time_zone = '-0:00';

CREATE TABLE tt ( t time(6), dt datetime(6), small_dt datetime(3), ts timestamp(4) NULL );

insert into tt set t = '12:23:33.222', dt = '2012-01-01 22:32:34.222222', small_dt = '2012-01-01 22:32:34.123';

-> { "database": "timedb", "table": "tt", "type": "insert", "data": {"t": "12:23:33.222000", "dt": "2012-01-01 22:32:34.222222", "small_dt": "2012-01-01 22:32:34.123" } }
